generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model cache {
  key        String @id
  value      String @db.MediumText
  expiration Int
}

model cache_locks {
  key        String @id
  owner      String
  expiration Int
}

model failed_jobs {
  id         BigInt   @id @default(autoincrement()) @db.UnsignedBigInt
  uuid       String   @unique(map: "failed_jobs_uuid_unique")
  connection String   @db.Text
  queue      String   @db.Text
  payload    String   @db.LongText
  exception  String   @db.LongText
  failed_at  DateTime @default(now()) @db.Timestamp(0)
}

model galleries {
  id         BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  title      String
  image      String
  category   String
  created_at DateTime? @db.Timestamp(0)
  updated_at DateTime? @db.Timestamp(0)
}

model heroes {
  id         BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  created_at DateTime? @db.Timestamp(0)
  updated_at DateTime? @db.Timestamp(0)
  title      String
  subtitle   String?
  image      String
  is_active  Boolean   @default(true)
}

model job_batches {
  id             String  @id
  name           String
  total_jobs     Int
  pending_jobs   Int
  failed_jobs    Int
  failed_job_ids String  @db.LongText
  options        String? @db.MediumText
  cancelled_at   Int?
  created_at     Int
  finished_at    Int?
}

model jobs {
  id           BigInt @id @default(autoincrement()) @db.UnsignedBigInt
  queue        String
  payload      String @db.LongText
  attempts     Int    @db.UnsignedTinyInt
  reserved_at  Int?   @db.UnsignedInt
  available_at Int    @db.UnsignedInt
  created_at   Int    @db.UnsignedInt

  @@index([queue], map: "jobs_queue_index")
}

model majors {
  id          BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  created_at  DateTime? @db.Timestamp(0)
  updated_at  DateTime? @db.Timestamp(0)
  name        String
  image       String?
  description String    @db.Text
}

model migrations {
  id        Int    @id @default(autoincrement()) @db.UnsignedInt
  migration String
  batch     Int
}

model news {
  id           BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  title        String
  slug         String    @unique(map: "news_slug_unique")
  content      String    @db.Text
  image        String?
  category     String
  author       String    @default("Admin")
  is_published Boolean   @default(true)
  is_featured  Boolean   @default(false)
  created_at   DateTime? @db.Timestamp(0)
  updated_at   DateTime? @db.Timestamp(0)
}

model partners {
  id         BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  name       String
  logo       String
  is_active  Boolean   @default(true)
  created_at DateTime? @db.Timestamp(0)
  updated_at DateTime? @db.Timestamp(0)
}

model password_reset_tokens {
  email      String    @id
  token      String
  created_at DateTime? @db.Timestamp(0)
}

model personal_access_tokens {
  id             BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  tokenable_type String
  tokenable_id   BigInt    @db.UnsignedBigInt
  name           String    @db.Text
  token          String    @unique(map: "personal_access_tokens_token_unique") @db.VarChar(64)
  abilities      String?   @db.Text
  last_used_at   DateTime? @db.Timestamp(0)
  expires_at     DateTime? @db.Timestamp(0)
  created_at     DateTime? @db.Timestamp(0)
  updated_at     DateTime? @db.Timestamp(0)

  @@index([expires_at], map: "personal_access_tokens_expires_at_index")
  @@index([tokenable_type, tokenable_id], map: "personal_access_tokens_tokenable_type_tokenable_id_index")
}

model registrants {
  id           BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  created_at   DateTime? @db.Timestamp(0)
  updated_at   DateTime? @db.Timestamp(0)
  name         String
  email        String
  phone        String
  major_choice String
  status       String    @default("pending")
}

model school_settings {
  id          BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  created_at  DateTime? @db.Timestamp(0)
  updated_at  DateTime? @db.Timestamp(0)
  school_name String    @default("SMK Digitech")
  navbar_logo String?
  address     String?
  phone       String?
  ppdb_open   Boolean   @default(true)
}

model sessions {
  id            String  @id
  user_id       BigInt? @db.UnsignedBigInt
  ip_address    String? @db.VarChar(45)
  user_agent    String? @db.Text
  payload       String  @db.LongText
  last_activity Int

  @@index([last_activity], map: "sessions_last_activity_index")
  @@index([user_id], map: "sessions_user_id_index")
}

model teachers {
  id          BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  name        String
  role        String
  subject     String
  experience  String
  level       String
  image       String
  theme_color String    @default("cyan")
  linkedin    String?
  twitter     String?
  email       String?
  created_at  DateTime? @db.Timestamp(0)
  updated_at  DateTime? @db.Timestamp(0)
}

model testimonis {
  id         BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  name       String
  role       String
  company    String
  content    String    @db.Text
  batch      String
  image      String?
  created_at DateTime? @db.Timestamp(0)
  updated_at DateTime? @db.Timestamp(0)
}

model users {
  id                BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  name              String
  email             String    @unique(map: "users_email_unique")
  email_verified_at DateTime? @db.Timestamp(0)
  password          String
  remember_token    String?   @db.VarChar(100)
  created_at        DateTime? @db.Timestamp(0)
  updated_at        DateTime? @db.Timestamp(0)
}

model Pengaduan {
  id        Int      @id @default(autoincrement())
  name      String?
  noHp      String?
  category  String
  urgency   String
  message   String
  image     String?
  createdAt DateTime @default(now())
}

model pengaduan {
  id        Int      @id @default(autoincrement())
  name      String?
  noHp      String?
  category  String
  urgency   String
  message   String
  createdAt DateTime @default(now())
  image     String?
}
